  "devDependencies": {
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.48.0",
    "eslint-config-custom": "workspace:*",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "typescript": "^4.9.5"
  }
